import math
x1 = int(input())
x2 = int(input())
y1 = int(input())
y2 = int(input())
distance = math.sqrt((x2 - x1)  2 + (y2 - y1)  2)
side_length = distance / math.sqrt(2)
area = side_length ** 2
perimeter = 4 * side_length
print(f"Площадь квадрата: {area}")
print(f"Периметр квадрата: {perimeter}")
