def fibonacci(n):
    if n == 0:
        return 0
    elif n == 1:
        return 0
    
    fib = [0, 1] 
    for i in range(2, n):
        fib.append(fib[i-1] + fib[i-2])
    return fib

n = 21
fib = fibonacci(n)
print()
for i, num in enumerate(fib):
    print(f"F{i} = {num}")

print(f"\nF20 = {fib[20]}")
