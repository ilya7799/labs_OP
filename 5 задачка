def fibonacci_sequence(n):
    """Генерирует первые n чисел Фибоначчи"""
    if n == 0:
        return []
    elif n == 1:
        return [0]
    
    fib = [0, 1]
    for i in range(2, n):
        fib.append(fib[i-1] + fib[i-2])
    return fib

# Генерируем первые 21 число (от F0 до F20)
n = 21
fib_sequence = fibonacci_sequence(n)
print(f"Первые {n} чисел Фибоначчи:")
for i, num in enumerate(fib_sequence):
    print(f"F_{i} = {num}")

print(f"\nF_20 = {fib_sequence[20]}")
